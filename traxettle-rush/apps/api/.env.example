# Firebase Admin SDK credentials
# Get these from: Firebase Console → Project Settings → Service accounts → Generate new private key
FIREBASE_PROJECT_ID=app-traxettle
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@app-traxettle.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
FIREBASE_STORAGE_BUCKET=app-traxettle.firebasestorage.app
FIREBASE_WEB_API_KEY=your-web-api-key

# Server
PORT=3001

# Web app URL (used as fallback in email links)
APP_URL=https://traxettle-staging.web.app

# Mobile app deep link scheme (used as primary link in email notifications)
MOBILE_APP_SCHEME=com.traxettle.app

# Email-link auth settings
AUTH_EMAIL_LINK_CONTINUE_URL=https://staging.traxettle.app/auth/email-link
AUTH_ANDROID_PACKAGE_NAME=com.traxettle.app
AUTH_ANDROID_MIN_VERSION=1
AUTH_IOS_BUNDLE_ID=com.traxettle.app
