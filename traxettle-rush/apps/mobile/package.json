{
  "name": "@traxettle/mobile",
  "version": "1.0.9",
  "description": "Traxettle mobile app â€” iOS & Android",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache .expo",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "build:ios": "eas build --platform ios --profile development",
    "build:android": "eas build --platform android --profile development",
    "build:android:debug": "bash scripts/build-android.sh debug",
    "build:android:debug:staging": "bash scripts/build-android.sh debug:staging",
    "build:android:staging": "bash scripts/build-android.sh staging",
    "build:android:production": "bash scripts/build-android.sh production",
    "build:ios:debug": "bash scripts/build-ios.sh debug",
    "build:ios:staging": "bash scripts/build-ios.sh staging",
    "build:ios:production": "bash scripts/build-ios.sh production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:maestro": "RUN_ID=${RUN_ID:-$(date +%s)} sh maestro/scripts/run-maestro.sh normal",
    "test:maestro:artifacts": "RUN_ID=${RUN_ID:-$(date +%s)} sh maestro/scripts/run-maestro.sh artifacts",
    "test:maestro:android": "RUN_ID=${RUN_ID:-$(date +%s)} MAESTRO_DEVICE=${MAESTRO_DEVICE:-emulator-5556} sh maestro/scripts/run-maestro.sh normal",
    "test:maestro:ios": "RUN_ID=${RUN_ID:-$(date +%s)} sh maestro/scripts/run-maestro.sh normal",
    "rush:dev:api": "cd ../.. && rush dev:api",
    "rush:dev:mobile": "cd ../.. && rush dev:mobile",
    "rush:test:mobile": "cd ../.. && rush test:mobile",
    "rush:test:mobile:coverage": "cd ../.. && rush test:mobile:coverage",
    "rush:test:e2e": "cd ../.. && rush test:e2e",
    "rush:test:maestro": "cd ../.. && rush test:maestro",
    "rush:test:maestro:artifacts": "cd ../.. && rush test:maestro:artifacts",
    "rush:qa:all": "cd ../.. && rush test:mobile:coverage && rush test:e2e && rush test:maestro"
  },
  "keywords": [
    "traxettle",
    "mobile",
    "react-native",
    "expo"
  ],
  "author": "Traxettle Team",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@react-native-google-signin/google-signin": "^12.2.0",
    "@react-native/community-cli-plugin": "0.81.5",
    "@react-native/gradle-plugin": "0.81.5",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^6.9.0",
    "@traxettle/shared": "workspace:^1.0.0",
    "expo": "~54.0.0",
    "expo-application": "~7.0.0",
    "expo-asset": "~12.0.0",
    "expo-auth-session": "~7.0.0",
    "expo-constants": "~18.0.0",
    "expo-crypto": "~15.0.0",
    "expo-file-system": "~19.0.0",
    "expo-font": "~14.0.0",
    "expo-image-picker": "~17.0.0",
    "expo-keep-awake": "~15.0.0",
    "expo-modules-core": "~3.0.0",
    "expo-print": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.0",
    "expo-web-browser": "~15.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-purchases": "^8.2.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "socket.io-client": "^4.7.0",
    "styled-components": "^6.1.0",
    "expo-intent-launcher": "~55.0.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.22",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.1.2",
    "@types/styled-components-react-native": "^5.2.0",
    "typescript": "^5.0.0",
    "babel-preset-expo": "~54.0.0"
  }
}
